{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"noEmit": true,
		"paths": {
			"types": ["../../../../types/internal"],
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"]
		}
	}
}
